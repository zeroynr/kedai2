{
	"$schema": "https://railway.app/railway.schema.json",
	"build": {
		"builder": "DOCKERFILE",
		"dockerfilePath": "Dockerfile" // Pastikan path Dockerfile sudah benar
	},
	"deploy": {
		"runtime": "V2",
		"numReplicas": 1,
		"sleepApplication": false,
		"restartPolicyType": "ON_FAILURE",
		"restartPolicyMaxRetries": 5
	}
}
